export ZSH="/home/justenb/.oh-my-zsh"
ZSH_THEME="cypher"

plugins=(git zsh-autosuggestions zsh-256color)


# envVars
export EDITOR='nvim'
export VISUAL='nvim'
export GOPATH='/home/justenb/code/go'
export TERM=xterm-256color
export ZPOOL_VDEV_NAME_PATH=YES
export PATH=$PATH:/home/justenb/bin

# aliases
alias vim="nvim"
alias alacritty="optirun alacritty"


source $ZSH/oh-my-zsh.sh


alias luamake=/home/justenb/.config/lua-language-server/3rd/luamake/luamake
